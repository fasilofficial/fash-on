  <body>
    <div class="screen-overlay"></div>
    <%- include('../partials/sidebar.ejs') %>
    <main class="main-wrap">
      <%- include('../partials/admin-header.ejs') %>
      <section class="content-main">
        <div class="content-header">
          <div>
            <h2 class="content-title card-title">Users List</h2>
            <p>Browse and Manage User Profiles</p>
          </div>
          <div>
            <a href="/admin/users/add" class="btn btn-primary btn-sm rounded"
              >Add user</a
            >
          </div>
        </div>
        <table class="table table-responsive table-striped">
          <thead>
            <tr>
              <th scope="col">#</th>
              <th scope="col">Full Name</th>
              <th scope="col">Email</th>
              <th scope="col">Phone</th>
              <th scope="col" class="text-end">Block/Unblock</th>
            </tr>
          </thead>
          <tbody>
            <% users.forEach((user, id) => { %>
            <tr>
              <th scope="row"><%= id + 1 %></th>
              <td><%= user.firstName + " " + user.lastName %></td>
              <td><%= user.email %></td>
              <td><%= "+91" + user.phone %></td>
              <td class="text-end">
                <form
                  action="/admin/users/edit/<%= user._id %>?_method=PUT"
                  method="POST"
                >
                  <% if (user.blocked) { %>
                  <button type="submit" class="btn btn-success">Unblock</button>
                  <% } else { %>
                  <button type="submit" class="btn btn-danger">Block</button>
                  <% } %>
                </form>
              </td>
            </tr>
            <% }) %>
          </tbody>
        </table>
